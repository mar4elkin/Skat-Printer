{
    "name": "SKAT Printer",
    "version": "2.0.0",
    "description": "Printer option for SKAT",
    "manifest_version": 3,
    "author": "Mark Tamarov",
    "action": {
        "default_title": "Skat Printer",   
        "default_popup": "popup.html"  
    },
    "background": {
        "service_worker": "build/background.js"
    },
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": ["<all_urls>"]
}